FoamFile
{
    format      ascii;
    class       volScalarField;
    object      fi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    borders
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

