FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    right
    {
        type            fixedValue;
        value           uniform 0;
    }
    "(borders|left)"
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

