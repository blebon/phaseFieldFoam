FoamFile
{
    format          ascii;
    class           volScalarField;
    object          fi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 0;
    }
    "(borders|right)"
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


